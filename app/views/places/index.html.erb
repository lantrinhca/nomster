<%= will_paginate @places, :class => "flickr_pagination" %><% @places.each	do |place| %>	<div class="booyah-box col-xs-10 col-xs-offset-1">		<h1><%= link_to place.name, place_path(place) %></h1><br>		<i><%= place.address %></i><br>		<p><%= place.description %></p><br>		<% if place.user.present? %>      <small><%= place.user.email %></small>    <% end %>		<% if place.comments.present? %>				<div class="col-xs-12">					<blockquote>						<% if place.comments.order("id ASC").last.humanized_rating.present? %>							<b><%= place.comments.order("id ASC").last.humanized_rating %></b> -						<% end %>						<%= place.comments.order("id ASC").last.message %>						<small><%= place.comments.order("id ASC").last.user.email %></small>					</blockquote>				</div>		<% end %>		<section id="photos">			<% if place.photos.present? && place.photos.last.picture.present? %>		    <p><%= image_tag place.photos.last.picture %></p><br>			<% end %>		</section>			</div><% end %>